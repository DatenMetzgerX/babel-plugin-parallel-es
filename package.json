{
  "name": "babel-plugin-parallel-es",
  "version": "0.1.8",
  "description": "Babel Plugin for Parallel.ES",
  "repository": "https://github.com/DatenMetzgerX/babel-plugin-parallel-es.git",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "report-dir": "./coverage",
    "reporter": [
      "lcov",
      "html",
      "text"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@types/babel-core": "^6.7.14",
    "@types/babel-generator": "^6.7.14",
    "@types/babel-template": "^6.7.14",
    "@types/babel-traverse": "^6.7.15",
    "@types/babel-types": "^6.7.14",
    "@types/babylon": "^6.7.15",
    "@types/source-map": "^0.5.0",
    "babel-code-frame": "^6.16.0",
    "babel-plugin-undeclared-variables-check": "^6.8.0",
    "source-map": "^0.5.6"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.45",
    "@types/sinon": "^1.16.31",
    "@types/sinon-chai": "^2.7.27",
    "babel-core": "^6.17.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "mocha": "^3.2.0",
    "nyc": "^10.0.0",
    "parallel-es": "^0.1.17",
    "prettier": "^1.11.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.9.0",
    "typescript": "^2.7.2"
  },
  "peerDependencies": {
    "babel-core": "^6.17.0"
  },
  "scripts": {
    "build": "tslint index.ts './src/**/*.ts' './test/**/*.ts' && tsc",
    "prepublish": "npm run-script build",
    "precommit": "lint-staged",
    "lint": "tslint --project .",
    "start": "tsc --watch",
    "test": "nyc mocha --inline-diffs './test/**/*.specs.ts'",
    "test:watch": "mocha --compilers ts:ts-node/register -w --inline-diffs './test/**/*.specs.ts' -w",
    "coveralljs": "cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "*.{js, json}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Micha Reiser <micha@famreiser.ch>",
  "license": "MIT"
}
