{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "only-arrow-functions": false,
    "ordered-imports": false,
    "no-string-literal": false,
    "adjacent-overload-signatures": false,
    "array-type": false,
    "arrow-parens": false,
    "max-classes-per-file": false
  }
}
